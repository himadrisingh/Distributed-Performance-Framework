    	 </nonstop>
    </terracotta>

  </cache>
  
  <cache name="ownerPets"
         maxElementsInMemory="${ownerPets_inMemory}"
         maxElementsOnDisk="${ownerPets_capacity}"
         eternal="false"
         overflowToDisk="false"
         diskSpoolBufferSizeMB="20"
