    	 </nonstop>
    </terracotta>

  </cache>
  
  <cache name="petTypes"
         maxElementsInMemory="${petTypes_inMemory}"
         maxElementsOnDisk="${petTypes_capacity}"
         eternal="false"
         overflowToDisk="false"
         diskSpoolBufferSizeMB="20"
